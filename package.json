{
  "name": "nodejs-ts-nosql-api",
  "version": "1.0.0",
  "description": "Boilerplate Project For TypeScript NodeJS REST API with MongoDB, OpenAPI Docs",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --respawn src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "lint": "eslint --ext .js,.ts ./src",
    "test": "test",
    "docs": "typedoc --entryPointStrategy expand ./src  --out docs --includeVersion --readme ./README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AfterWorkx/nodejs-ts-nosql-api.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "mongodb",
    "openapi",
    "mocha"
  ],
  "author": "TheBumpaster",
  "license": "SEE LICENSE IN LICENSE ",
  "bugs": {
    "url": "https://github.com/AfterWorkx/nodejs-ts-nosql-api/issues"
  },
  "homepage": "https://github.com/AfterWorkx/nodejs-ts-nosql-api#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "i18n": "^0.15.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.2",
    "multer": "^1.4.5-lts.1",
    "socket.io-client": "^4.5.1",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/deepmerge": "^2.2.0",
    "@types/express": "^4.17.13",
    "@types/i18n": "^0.13.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.0.0",
    "mongodb-memory-server": "^8.8.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  }
